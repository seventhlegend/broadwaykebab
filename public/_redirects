# Exact static file paths - priority over fallbacks
/_next/static/css/* /_next/static/css/:splat 200
/_next/static/chunks/app/* /_next/static/chunks/app/:splat 200
/_next/static/chunks/* /_next/static/chunks/:splat 200
/_next/static/* /_next/static/:splat 200

# Menu page routing
/menu /menu/index.html 200
/menu/ /menu/index.html 200

# 404 fallback
/404 /404.html 200

# SPA fallback for any other routes - LAST
/* /index.html 200
