{
  "name": "broadwaykebab",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && cp public/_headers out/ && cp public/_redirects out/ && mkdir -p out/_functions && cp \"public/_functions/[[path]].js\" out/_functions/",
    "start": "next start",
    "lint": "next lint",
    "export": "next build",
    "serve": "npx serve out",
    "deploy:github": "npm run build && touch out/.nojekyll",
    "deploy:netlify": "npm run build",
    "deploy:vercel": "npm run build",
    "deploy:cloudflare": "npm run build && echo 'Upload out/ directory to Cloudflare Pages'"
  },
  "dependencies": {
    "@tailwindcss/line-clamp": "^0.4.4",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.511.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
